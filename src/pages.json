{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/tabBar/component",
			"style": {
				"navigationBarTitleText": "组件"
			}
		},
		{
			"path": "pages/tabBar/extend",
			"style": {
				"navigationBarTitleText": "扩展"
			}
		},
		{
			"path": "pages/tabBar/template",
			"style": {
				"navigationBarTitleText": "模板"
			}
		}
	],
	"subPackages": [
		{
			"root": "pages/template",
			"pages": [
				{
					"path": "f2-chart",
					"style": {
						"navigationBarTitleText": "F2报表",
						"navigationStyle": "default"
					}
				},
				{
					"path": "ucharts",
					"style": {
						"navigationBarTitleText": "uCharts报表",
						"navigationStyle": "default"
					}
				},
				{
					"path": "sub-select",
					"style": {
						"navigationBarTitleText": "多级弹出菜单",
						"navigationStyle": "default"
					}
				},
				{
					"path": "float-button",
					"style": {
						"navigationBarTitleText": "浮动按钮",
						"navigationStyle": "default"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#007AFF",
		"borderStyle": "black",
		"backgroundColor": "#F8F8F8",
		"list": [
			{
				"pagePath": "pages/tabBar/component",
				"iconPath": "static/component.png",
				"selectedIconPath": "static/componentHL.png",
				"text": "组件"
			},
			{
				"pagePath": "pages/tabBar/extend",
				"iconPath": "static/extui.png",
				"selectedIconPath": "static/extuiHL.png",
				"text": "扩展"
			},
			{
				"pagePath": "pages/tabBar/template",
				"iconPath": "static/template.png",
				"selectedIconPath": "static/templateHL.png",
				"text": "模板"
			}
		]
	}
}